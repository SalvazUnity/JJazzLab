    <artifactId>org-jjazz-ss_editor</artifactId>
    <name>SS_Editor</name>
    <packaging>nbm</packaging>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.netbeans.utilities</groupId>
                <artifactId>nbm-maven-plugin</artifactId>
                <configuration>
                    <publicPackages>                      
                        <publicPackage>org.jjazz.ss_editor.actions</publicPackage>
                        <publicPackage>org.jjazz.ss_editor.api</publicPackage>
                        <publicPackage>org.jjazz.ss_editor.rpviewer.api</publicPackage>
                        <publicPackage>org.jjazz.ss_editor.rpviewer.spi</publicPackage>
                        <publicPackage>org.jjazz.ss_editor.spi</publicPackage>
                        <publicPackage>org.jjazz.ss_editor.sptviewer.api</publicPackage>
                        <publicPackage>org.jjazz.ss_editor.sptviewer.spi</publicPackage>
                    </publicPackages>
                </configuration>
                <extensions>true</extensions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <configuration>
                    <archive>
                        <manifestFile>${project.build.outputDirectory}/META-INF/MANIFEST.MF</manifestFile>
                    </archive>
                </configuration>
            </plugin>
        </plugins>
    </build>
